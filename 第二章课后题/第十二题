int count(LinkList clist)

     int count;
     
     PNode q;
     
     if(clist==NULL) return 0;
     
     q=clist->link;
     
     count =1;
     
     while(q !=clist){
     
     
              q=q->link;
              
              count++;
              
     }
 
     return count;
     
 }
